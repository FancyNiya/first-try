
def move(n, a, b, c): 
    if n == 1:
          print('move', a, '-->', c)
          return
    move(n-1, a, c, b) 
    print('move', a, '-->', c)
    move(n-1, b, a, c) 
def move(n-1, b, a, c):
      if n-1 == 1:
             print('move', b, '-->', c )
             return
     move(n-2, b, c, a)  
     print('move', b, '-->', c)
     move(n-2, a, b, c) 

